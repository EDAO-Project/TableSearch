, {"text": "Numerous war crimes committed during the Bosnian war by the Serb political and military leadership mostly on Bosniak civilians in the Prijedor region of Bosnia-Herzegovina.", "isNumeric": false, "links": []}], [{"text": "Vi\u0161egrad massacre", "isNumeric": false, "links": ["http://www.wikipedia.org/wiki/Vi\u0161egrad_massacres"]}, {"text": "Crime of genocide, Crimes against humanity (Murder of over 3,000 civilians)", "isNumeric": false, "links": []}, {"text": "Serbian police and military forces. Seven officers convicted.", "isNumeric": false, "links": []}, {"text": "Acts of ethnic cleansing and mass murder of Bosniak civilians that occurred in the town of Vi\u0161egrad in eastern Bosnia and Herzegovina, committed by Serb police and military forces at the start of the Bosnian War during the spring of 1992.", "isNumeric": false, "links": []}], [{"text": "Fo\u010da massacres", "isNumeric": false, "links": ["http://www.wikipedia.org/wiki/Fo\u010da_massacres"]}, {"text": "Crime of genocide, Crimes against humanity (Murder of over 2,704 civilians)", "isNumeric": false, "links": []}, {"text": "Army of Republika Srpska. Eight officers and soldiers convicted.", "isNumeric": false, "links": []}, {"text": "A series of killings committed by Serb military, police and paramilitary forces on Bosniak civilians in the Fo\u010da region of Bosnia-Herzegovina (including the towns of Gacko and Kalinovik) from April 7, 1992 to January, 1994. In numerous verdicts, the International Criminal Tribunal for the former Yugoslavia ruled that these killings constituted crimes against humanity and acts of genocide.", "isNumeric": false, "links": []}], [{"text": "Markale massacre", "isNumeric": false, "links": ["http://www.wikipedia.org/wiki/Markale_massacres"]}, {"text": "Crimes against humanity (Murder of 105 civilians and wounding 234)", "isNumeric": false, "links": []}, {"text": "Stanislav Gali\u0107", "isNumeric": false, "links": ["http://www.wikipedia.org/wiki/Stanislav_Gali\u0107"]}, {"text": "Sarajevo", "isNumeric": false, "links": ["http://www.wikipedia.org/wiki/Sarajevo"]}], [{"text": "Siege of Sarajevo", "isNumeric": false, "links": ["http://www.wikipedia.org/wiki/Siege_of_Sarajevo"]}, {"text": "Crimes against humanity, (10,000 civilians killed)", "isNumeric": false, "links": []}, {"text": "Stanislav Gali\u0107", "isNumeric": false, "links": ["http://www.wikipedia.org/wiki/Stanislav_Gali\u0107"]}, {"text": "The longest siege of a capital city in the history of modern warfare. Republika Srpska and the Yugoslav People's Army besieged Sarajevo, the capital city of Bosnia and Herzegovina, from April 5, 1992 to February 29, 1996.", "isNumeric": false, "links": []}], [{"text": "Siege of Biha\u0107", "isNumeric": false, "links": ["http://www.wikipedia.org/wiki/Siege_of_Biha\u0107"]}, {"text": "Crimes against humanity", "isNumeric": false, "links": []}, {"text": "Army of Republika Srpska.", "isNumeric": false, "links": []}, {"text": "From April 1992 to August 1995.", "isNumeric": false, "links": []}], [{"text": "Tuzla massacre", "isNumeric": false, "links": ["http://www.wikipedia.org/wiki/Tuzla_massacre"]}, {"text": "Crimes against humanity (Murder of 72 and wounding of more than 200 individuals)", "isNumeric": false, "links": []}, {"text": "Novak \u0110uki\u0107", "isNumeric": false, "links": []}, {"text": "Tuzla", "isNumeric": false, "links": ["http://www.wikipedia.org/wiki/Tuzla"]}], [{"text": "Kori\u0107ani Cliffs massacre", "isNumeric": false, "links": ["http://www.wikipedia.org/wiki/Kori\u0107ani_Cliffs_massacre"]}, {"text": "Crimes against humanity, War crimes (Murder of over 200 men)", "isNumeric": false, "links": []}, {"text": "Darko Mr\u0111a", "isNumeric": false, "links": []}, {"text": "Mass murder of more than 200 Bosniak men on 21 August 1992 at the Kori\u0107ani Cliffs (Kori\u0107anske Stijene) location on Mount Vla\u0161i\u0107, Bosnia and Herzegovina", "isNumeric": false, "links": []}], [{"text": "Ahatovi\u0107i massacre", "i